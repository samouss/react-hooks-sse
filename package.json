{
  "name": "react-hook-sse",
  "version": "1.0.0",
  "description": "React Hook for SSE",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:umd",
    "build:cjs": "babel src --out-dir dist/cjs",
    "build:esm": "BABEL_ENV=esm babel src --out-dir dist/es",
    "build:umd": "BABEL_ENV=esm rollup -c rollup.config.js",
    "watch": "yarn build:cjs --watch",
    "lint": "eslint src",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "start:server": "node server.js",
    "start:example": "(cd example && yarn && yarn start)",
    "source:link": "scripts/link.sh",
    "source:unlink": "scripts/unlink.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "cors": "2.8.4",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.0",
    "express": "4.16.4",
    "jest": "23.6.0",
    "jest-watch-typeahead": "0.2.0",
    "prettier": "1.15.3",
    "react": "16.7.0-alpha.0",
    "rollup": "0.67.3",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-terser": "3.0.0",
    "uuid": "3.3.2"
  },
  "peerDependencies": {
    "react": ">= 16.7.0-alpha.0"
  }
}
